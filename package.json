{
  "name": "antd-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Ant Design component",
  "type": "module",
  "bin": "dist/cli.js",
  "scripts": {
    "start": "node dist/cli.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "pretest": "tsup",
    "extract": "tsx cli.ts extract",
    "inspector": "npm run build & npx @modelcontextprotocol/inspector node dist/cli.js",
    "generate-component-changelog": "tsx src/scripts/generate.component-changelog.ts",
    "crawl-practices": "tsx cli.ts crawl-practices",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "componentData",
    "README.md",
    "README.zh-CN.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "zod": "^3.22.4",
    "to-vfile": "^8.0.0",
    "vfile-matter": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ant-design",
    "antd",
    "ui-components"
  ],
  "author": "luxiaolong",
  "license": "MIT"
} 